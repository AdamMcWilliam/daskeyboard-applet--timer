  "dependencies": {
    "daskeyboard-applet": "^2.11.4"
  },
  "qConfig": {
    "geometry": {
      "width": 1,
      "height": 1,
      "origin": {
        "x": 1,
        "y": 1
      }
    },
    "questions": [
      {
        "key": "seconds",
        "label": "Choose how many seconds",
        "help": "",
        "required": true,
        "order": 1,
        "value": "10",
        "controlType": "textbox"
      },
      {
        "key": "minutes",
        "label": "Choose how many minutes",
        "help": "",
        "required": true,
        "order": 2,
        "value": "10",
        "controlType": "textbox"
      },
      {
        "key": "hours",
        "label": "Choose how many hours",
        "help": "",
        "required": true,
        "order": 3,
        "value": "10",
        "controlType": "textbox"
      }
    ],
    "requirements": {
      "applet": {}
    }
  },
  "devDependencies": {
    "mocha": "^5.2.0"
  }
